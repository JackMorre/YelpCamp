<% layout("/layouts/boilerplate")%>
<div class="row">
	<div class="col-xl-12">
		<div
			class="alert alert-danger"
			role="alert"
		>
			<h4 class="alert-heading"><%=err.message%></h4>
			<% if(process.env.NODE_ENV !== "production") {%>
			<p class="text-wrap"><%= err.stack %></p>
			<% } %>
			<hr />
		</div>
	</div>
</div>
